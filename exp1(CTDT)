% ylabel('Amplitude');

%unit ramp signal


% t1 = -10:0.01:10;
% t2 = 10:0.01:20;
% u1= zeros(size(t1));
% u3=t2;
% u5=[u1,u3]
% T1 = [t1,t2];
% figure;
% plot(T1,u5);
% title('unit step signal continous time');
% xlabel('time');
% ylabel('Amplitude');

% n1 = -10:0.1:10;
% n2= 10:0.1:20;
% u2=zeros(size(n1));
% u4 = n2;
% u6= [u2,u4];
% N2=[n1,n2];
% figure;
% stem(N2,u6);
% title('unit step signal discrete time');
% xlabel('time');
% ylabel('Amplitude');

