% cos signal ct and dt
% t = 0:0.01:10;
% x1= cos(t);
% subplot(5,2,1);
% plot(t,x1);
% title('Cos signal continous time');
% xlabel('time');
% ylabel('Amplitude');
% 
% n = 0:0.1:10;
% x2= cos(n);
% subplot(5,2,2);
% stem(n,x2);
% title('Cos signal discrete time');
% xlabel('time');
% ylabel('Amplitude');
% 
% %y(t) signal ct and dt
% 
% t = 0:0.01:10;
% y1= 3*cos(2*pi*t+pi/3);
% subplot(5,2,3);
% plot(t,y1);
% title('y(t) signal continous time');
% xlabel('time');
% ylabel('Amplitude');
% 
% n = 0:0.1:10;
% y2= 3*cos(2*pi*n+pi/3);
% subplot(5,2,4);
% stem(n,y2);
% title('y(n) signal discrete time');
% xlabel('time');
% ylabel('Amplitude');
