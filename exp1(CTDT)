% 
% %exponential signal
% 
% t = 0:0.01:10;
% u1= 3*exp(0.4*t);
% subplot(5,2,5);
% plot(t,u1);
% title('exponential signal continous time');
% xlabel('time');
% ylabel('Amplitude');
% 
% n = 0:0.1:10;
% u2= 3*exp(0.4*n);
% subplot(5,2,6);
% stem(n,u2);
% title('exponential signal discrete time');
